#pragma kernel Copy 

RWTexture3D<float4> _Atlas;

[numthreads(8,8,8)]
void Copy (uint3 id : SV_DispatchThreadID)
{
    // TODO: copy code + option for opld alpha8 textures to copy them with correct data (1, 1, 1, a)

    _Atlas[id.xyz] = float4(id.x & id.y, (id.x & 15)/15.0, (id.y & 15)/15.0, 0.0);
}
